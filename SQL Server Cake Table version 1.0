--creates a table named cakes in the database 
CREATE TABLE CAKES ( 
CAKE_TYPE TEXT,
CAKE_FLAVOR TEXT,  
ICING_FLAVOR TEXT, 
CAKE_PRICE MONEY
) 
-- use to verify table was created, also use throughout insert statements to verify data was inserted 
SELECT * 
FROM CAKES 

--use drop command to delete table 
DROP TABLE CAKES 

